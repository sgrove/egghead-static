    "build": "rm -rf public && bsb -clean-world && bsb -make-world && gatsby build --prefix-paths && yarn move",
    "move": "cd public && mkdir learn | mv * learn",
    "refresh": "node loadStaticDataToFiles",
    "develop": "gatsby develop",
    "start": "serve public/",
    "now-build": "npm run build",
    "reason": "bsb -clean-world && bsb -make-world -w",
